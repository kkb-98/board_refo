<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                       http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
 	<servlet>
 		<servlet-name>detail</servlet-name>
 		<servlet-class>com.bit.controller.DetailController</servlet-class>
 	</servlet>
 	<servlet-mapping>
  	<servlet-name>detail</servlet-name>
  	<url-pattern>/detail.do</url-pattern>
  </servlet-mapping>
  
  
 	<servlet>
 		<servlet-name>edit</servlet-name>
 		<servlet-class>com.bit.controller.EditController</servlet-class>
 	</servlet>
 	<servlet-mapping>
  	<servlet-name>edit</servlet-name>
  	<url-pattern>/edit.do</url-pattern>
  </servlet-mapping>
  
 	<servlet>
 		<servlet-name>delete</servlet-name>
 		<servlet-class>com.bit.controller.DeleteController</servlet-class>
 	</servlet>
 	<servlet-mapping>
  	<servlet-name>delete</servlet-name>
  	<url-pattern>/delete.do</url-pattern>
  </servlet-mapping>
  
  
 	
 	<servlet>
  <servlet-name>list</servlet-name>
  	<servlet-class>com.bit.controller.ListController</servlet-class>
  </servlet>
 	<servlet-mapping>
  	<servlet-name>list</servlet-name>
  	<url-pattern>/list.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>add</servlet-name>
  	<servlet-class>com.bit.controller.AddController</servlet-class>
  </servlet>
  
 	<servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add.do</url-pattern>
  </servlet-mapping>
  

</web-app>
